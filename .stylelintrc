{
  "extends": [
    "stylelint-config-standard",
    "stylelint-config-recommended",
    "stylelint-config-rational-order",
    "stylelint-config-prettier"
  ],
  "rules": {
    "no-missing-end-of-source-newline": null,
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "include",
        "mixin",
        "function",
        "return",
        "if",
        "warn",
        "error",
        "else",
        "for",
        "each",
        "debug"
      ]
    }],
  }
}